// Online C++ compiler to run C++ program online
#include <iostream>
#include <string>
using namespace std;

class Car {
    
private:
    int year;
    string model;
    string colour;

public:
    //constructer 
    Car(int y, string m, string c) : year(y), model(m), colour(c) {}
    //getter
    int getYear() {return year;}
    string getModel() {return model;}
    string getColour() {return colour;}
    //setter
    void setYear(int a) {year = a;}
    void setModel(string b) {model = b;}
    void setColour(string c) {colour = c;}
};

int main(){
    Car car(2012,"honda","red");
    cout << "This is car " << car.getYear() << " " << car.getModel() << " " << car.getColour() << endl;
}



